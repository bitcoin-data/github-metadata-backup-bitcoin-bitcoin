        "issue": {
          "id": 3247974,
          "node_id": "MDU6SXNzdWUzMjQ3OTc0",
          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/846",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/846/labels%7B/name%7D",
