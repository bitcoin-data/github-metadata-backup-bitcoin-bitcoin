      "tree": {
        "sha": "5b0a9e633eb53544fb561c15fdc9c07a35a4e6eb",
        "url": "https://api.github.com/repos/bitcoin/bitcoin/git/trees/5b0a9e633eb53544fb561c15fdc9c07a35a4e6eb"
      },
      "verification": {
        "verified": true,
