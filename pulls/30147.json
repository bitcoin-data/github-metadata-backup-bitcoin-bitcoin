      "tree": {
        "sha": "54cbe5c1236f51998bfd828e57c9e2f5d313b4bb",
        "url": "https://api.github.com/repos/bitcoin/bitcoin/git/trees/54cbe5c1236f51998bfd828e57c9e2f5d313b4bb"
