    "title": "p2p: Allow block downloads from peers without snapshot block after assumeutxo validation",
    "user": {
      "login": "stringintech",
      "id": 157148846,
      "node_id": "U_kgDOCV3mrg",
      "avatar_url": "https://avatars.githubusercontent.com/u/157148846?v=4",
      "gravatar_id": "",
