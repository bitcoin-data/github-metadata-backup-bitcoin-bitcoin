          "id": 213443031,
          "node_id": "MDExOlB1bGxSZXF1ZXN0MTEwMjAyNjQ3",
          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/9972",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/9972/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/9972/comments",
