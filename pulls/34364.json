    "title": "contrib: add initial zsh completion for bitcoin-cli",
    "user": {
      "login": "crackedhandle",
      "id": 195676843,
      "node_id": "U_kgDOC6nKqw",
      "avatar_url": "https://avatars.githubusercontent.com/u/195676843?v=4",
      "gravatar_id": "",
      "url": "https://api.github.com/users/crackedhandle",
      "html_url": "https://github.com/crackedhandle",
      "followers_url": "https://api.github.com/users/crackedhandle/followers",
      "following_url": "https://api.github.com/users/crackedhandle/following%7B/other_user%7D",
      "gists_url": "https://api.github.com/users/crackedhandle/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/crackedhandle/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/crackedhandle/subscriptions",
      "organizations_url": "https://api.github.com/users/crackedhandle/orgs",
      "repos_url": "https://api.github.com/users/crackedhandle/repos",
      "events_url": "https://api.github.com/users/crackedhandle/events%7B/privacy%7D",
      "received_events_url": "https://api.github.com/users/crackedhandle/received_events",
