      "html_url": "https://github.com/deadmanoz",
      "followers_url": "https://api.github.com/users/deadmanoz/followers",
      "following_url": "https://api.github.com/users/deadmanoz/following%7B/other_user%7D",
      "gists_url": "https://api.github.com/users/deadmanoz/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/deadmanoz/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/deadmanoz/subscriptions",
      "organizations_url": "https://api.github.com/users/deadmanoz/orgs",
      "repos_url": "https://api.github.com/users/deadmanoz/repos",
