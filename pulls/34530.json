      "url": "https://api.github.com/users/DukeDeSouth",
      "html_url": "https://github.com/DukeDeSouth",
      "followers_url": "https://api.github.com/users/DukeDeSouth/followers",
      "following_url": "https://api.github.com/users/DukeDeSouth/following%7B/other_user%7D",
      "gists_url": "https://api.github.com/users/DukeDeSouth/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/DukeDeSouth/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/DukeDeSouth/subscriptions",
      "organizations_url": "https://api.github.com/users/DukeDeSouth/orgs",
      "repos_url": "https://api.github.com/users/DukeDeSouth/repos",
      "events_url": "https://api.github.com/users/DukeDeSouth/events%7B/privacy%7D",
      "received_events_url": "https://api.github.com/users/DukeDeSouth/received_events",
